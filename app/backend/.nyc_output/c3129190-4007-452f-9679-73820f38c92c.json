   }\n\n    return this._model.findByIdAndDelete({ _id });\n  }\n}\n\nexport default MongoModel;\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"d8e13ff9eb9a7dcefb6ab7fac5cb4bd329c4b142"}}